{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Pico Debug (Cortex-Debug Linux)",
            "cwd": "${userHome}/.pico-sdk/openocd/0.12.0+dev/scripts",
            "executable": "${workspaceFolder}/build/sample1",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "serverpath": "${userHome}/.pico-sdk/openocd/0.12.0+dev/openocd.exe",
            "gdbPath": "gdb-multiarch",
            "device": "RP2040",
            "configFiles": [
                "interface/cmsis-dap.cfg",
                "target/rp2040.cfg"
            ],
            "svdFile": "${userHome}/.pico-sdk/sdk/2.0.0/src/rp2040/hardware_regs/RP2040.svd",
            "runToEntryPoint": "sta_ker",
            "overrideLaunchCommands": [
                "monitor reset init",
                "load \"${workspaceFolder}/build/sample1\""
            ],
            "openOCDLaunchCommands": [
                "adapter speed 5000"
            ]
        },
        {
            "name": "Pico Debug (Cortex-Debug Windows)",
            "cwd": "${userHome}/.pico-sdk/openocd/0.12.0+dev/scripts",
            "executable": "${workspaceFolder}/build/sample1",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "openocd",
            "serverpath": "${userHome}/.pico-sdk/openocd/0.12.0+dev/openocd.exe",
            "gdbPath": "${userHome}/.pico-sdk/toolchain/13_2_Rel1/bin/arm-none-eabi-gdb",
            "device": "RP2040",
            "configFiles": [
                "interface/cmsis-dap.cfg",
                "target/rp2040.cfg"
            ],
            "svdFile": "${userHome}/.pico-sdk/sdk/2.0.0/src/rp2040/hardware_regs/RP2040.svd",
            "runToEntryPoint": "sta_ker",
            "overrideLaunchCommands": [
                "monitor reset init",
                "load \"${workspaceFolder}/build/sample1\""
            ],
            "openOCDLaunchCommands": [
                "adapter speed 5000"
            ]
        }
    ]
}